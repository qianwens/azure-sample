{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "args": [
                "runserver",
                "--noreload"
            ],
            "django": true,
            "env": {
                "DBHOST": "{DBHOST}",
                "DBUSER": "{DBUSER}",
                "DBNAME": "{DBNAME}",
                "DBPASS": "{DBPASS}",
            }
        }
    ],
}